{% extends 'base.html.twig' %}

{% block title %}AdjustStocks index{% endblock %}

{% block body %}
    <h1>AdjustStocks index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Raison</th>
                <th>Notes</th>
                <th>Type</th>
                <th>CreatedAt</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for adjust_stock in adjust_stocks %}
            <tr>
                <td>{{ adjust_stock.id }}</td>
                <td>{{ adjust_stock.Raison }}</td>
                <td>{{ adjust_stock.notes }}</td>
                <td>{{ adjust_stock.type }}</td>
                <td>{{ adjust_stock.createdAt ? adjust_stock.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('adjust_stocks_show', {'id': adjust_stock.id}) }}">show</a>
                    <a href="{{ path('adjust_stocks_edit', {'id': adjust_stock.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('adjust_stocks_new') }}">Create new</a>
{% endblock %}
